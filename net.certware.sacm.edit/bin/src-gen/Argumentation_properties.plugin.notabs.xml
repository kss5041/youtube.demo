<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

 <plugin>

  <!-- EEF extensions -->
   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="http://schema.omg.org/SACM/1.0/Argumentation"
            class="net.certware.sacm.SACM.Argumentation.providers.ArgumentationEEFAdapterFactory"
            supportedTypes="org.eclipse.emf.eef.runtime.providers.PropertiesEditingProvider"/>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="net.certware.sacm.SACM.providers.SACMPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="net.certware.sacm.SACM.Evidence.providers.EvidencePropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="net.certware.sacm.SACM.Argumentation.providers.ArgumentationPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>

  <!-- Tabbed properties views extension -->
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="net.certware.sacm.SACM.Argumentation.properties"
            labelProvider="org.eclipse.emf.eef.runtime.ui.utils.EEFLabelProvider">
         <propertyCategory
               category="default">
         </propertyCategory>
         <propertyCategory
               category="extended">
         </propertyCategory>
         <propertyCategory
               category="advanced">
         </propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="net.certware.sacm.SACM.Argumentation.properties">
         <propertyTab
               id="Base"
               label="Base"
               category="default">
         </propertyTab>

      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="net.certware.sacm.SACM.Argumentation.properties">
         <propertySection
               class="net.certware.sacm.SACM.Argumentation.parts.forms.ArgumentationPropertiesEditionPartForm"
               filter="net.certware.sacm.SACM.Argumentation.providers.ArgumentationPropertiesEditionProvider$EditionFilter"
               id="net.certware.sacm.SACM.Argumentation.section.Argumentation"
               tab="Base">
         </propertySection>
         <propertySection
               class="net.certware.sacm.SACM.Argumentation.parts.forms.CitationElementPropertiesEditionPartForm"
               filter="net.certware.sacm.SACM.Argumentation.providers.CitationElementPropertiesEditionProvider$EditionFilter"
               id="net.certware.sacm.SACM.Argumentation.section.CitationElement"
               tab="Base">
         </propertySection>
         <propertySection
               class="net.certware.sacm.SACM.Argumentation.parts.forms.InformationElementPropertiesEditionPartForm"
               filter="net.certware.sacm.SACM.Argumentation.providers.InformationElementPropertiesEditionProvider$EditionFilter"
               id="net.certware.sacm.SACM.Argumentation.section.InformationElement"
               tab="Base">
         </propertySection>
         <propertySection
               class="net.certware.sacm.SACM.Argumentation.parts.forms.ArgumentReasoningPropertiesEditionPartForm"
               filter="net.certware.sacm.SACM.Argumentation.providers.ArgumentReasoningPropertiesEditionProvider$EditionFilter"
               id="net.certware.sacm.SACM.Argumentation.section.ArgumentReasoning"
               tab="Base">
         </propertySection>
         <propertySection
               class="net.certware.sacm.SACM.Argumentation.parts.forms.ClaimPropertiesEditionPartForm"
               filter="net.certware.sacm.SACM.Argumentation.providers.ClaimPropertiesEditionProvider$EditionFilter"
               id="net.certware.sacm.SACM.Argumentation.section.Claim"
               tab="Base">
         </propertySection>
         <propertySection
               class="net.certware.sacm.SACM.Argumentation.parts.forms.AssertedInferencePropertiesEditionPartForm"
               filter="net.certware.sacm.SACM.Argumentation.providers.AssertedInferencePropertiesEditionProvider$EditionFilter"
               id="net.certware.sacm.SACM.Argumentation.section.AssertedInference"
               tab="Base">
         </propertySection>
         <propertySection
               class="net.certware.sacm.SACM.Argumentation.parts.forms.AssertedEvidencePropertiesEditionPartForm"
               filter="net.certware.sacm.SACM.Argumentation.providers.AssertedEvidencePropertiesEditionProvider$EditionFilter"
               id="net.certware.sacm.SACM.Argumentation.section.AssertedEvidence"
               tab="Base">
         </propertySection>
         <propertySection
               class="net.certware.sacm.SACM.Argumentation.parts.forms.AssertedContextPropertiesEditionPartForm"
               filter="net.certware.sacm.SACM.Argumentation.providers.AssertedContextPropertiesEditionProvider$EditionFilter"
               id="net.certware.sacm.SACM.Argumentation.section.AssertedContext"
               tab="Base">
         </propertySection>
         <propertySection
               class="net.certware.sacm.SACM.Argumentation.parts.forms.AssertedChallengePropertiesEditionPartForm"
               filter="net.certware.sacm.SACM.Argumentation.providers.AssertedChallengePropertiesEditionProvider$EditionFilter"
               id="net.certware.sacm.SACM.Argumentation.section.AssertedChallenge"
               tab="Base">
         </propertySection>
         <propertySection
               class="net.certware.sacm.SACM.Argumentation.parts.forms.AssertedCounterEvidencePropertiesEditionPartForm"
               filter="net.certware.sacm.SACM.Argumentation.providers.AssertedCounterEvidencePropertiesEditionProvider$EditionFilter"
               id="net.certware.sacm.SACM.Argumentation.section.AssertedCounterEvidence"
               tab="Base">
         </propertySection>
      </propertySections>
   </extension>
 </plugin>
